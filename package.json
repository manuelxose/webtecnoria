  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "npm run build:ssr:debug && node --require ./server-dom-shim.js dist/landrick-angular/server/main.js",
    "build:ssr": "npm run build:prod && ng run landrick-angular:server:production",
    "build:ssr:debug": "ng build && ng run landrick-angular:server",
    "serve:ssr": "node --require ./server-dom-shim.js dist/landrick-angular/server/main.js",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "prerender": "ng run landrick-angular:prerender",
    "deploy:firebase": "npm run build:ssr && npm run copy:dist && cd functions && npm install && cd .. && firebase deploy",
    "copy:dist": "rimraf functions/dist && xcopy /E /I /Y dist functions\\dist",
    "clean": "rimraf dist && rimraf .angular && rimraf node_modules/.cache",
    "postinstall": "ngcc --properties es2015 browser module main --first-only"
  },
